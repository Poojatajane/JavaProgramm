package ListProgramm

import java.util.ArrayList ;

public class List1 {

public static void main(String[]args) {

ArrayList <String> al = new ArrayList<String>();
al.add("Apple");
al.add("Ball");
al.add("Cat");
al.add("Dog");
al.add("Elephant");
al.add("Fish");
System.out.println("List:"+al);

System.out.println();

System.out.println("List print using for loop:");

for(int i = 0 ; i <5; i++)
{
  String getElement = al.get(i);
  System.out.println(i);
}
 System.out.println("Element at index 4:");
 String getE = al.get(4);
 System.out.println(getE);

  }
}
